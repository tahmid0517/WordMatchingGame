<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tahmid.wordmatchinggame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".MainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".KeysPage" />
        <activity android:name=".TwoByTwoGrid" />
        <activity android:name=".VictoryPage" />
        <activity android:name=".TwoByThreeGrid" />
        <activity android:name=".TwoByFourGrid" />
        <activity android:name=".TwoByFiveGrid" />
        <activity android:name=".TwoBySixGrid" />
        <activity android:name=".EndGame"></activity>
    </application>

</manifest>